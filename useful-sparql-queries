PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
      PREFIX ddiem: <http://ddiem.phenomebrowser.net/>
      PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
      PREFIX owl: <http://www.w3.org/2002/07/owl#>
      PREFIX dc: <http://purl.org/dc/elements/1.1/>
      
      SELECT count(?disease) count(?drug) count(?procedure) count(?evidence) count(?reference) count(?classes)
      FROM <http://www.cbrc.kaust.edu.sa/DDIEM>
      WHERE {
        ?disease rdf:type ddiem:Disease .
        ?drug rdf:type ddiem:Drug .
        ?procedure rdf:type ddiem:TheraputicProcedure .
        ?evidence rdf:type ddiem:Evidence . 
        ?subj dc:provenance ?reference. 
        ?classes rdf:type  owl:Class 
      }

PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
      PREFIX ddiem: <http://ddiem.phenomebrowser.net/>
      PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
      PREFIX owl: <http://www.w3.org/2002/07/owl#>
      PREFIX dc: <http://purl.org/dc/elements/1.1/>
      
      SELECT (count(distinct ?disease) as ?disease_count) (count(distinct ?drug) as ?drug_count)
      FROM <http://ddiem.phenomebrowser.net>
      WHERE {
        ?disease rdf:type ddiem:Disease .
        ?drug rdf:type ddiem:Drug .
      }

SELECT (count(distinct ?procedure) as ?procedure_count) (count(distinct ?reference) as ?reference_count)
      FROM <http://ddiem.phenomebrowser.net>
      WHERE {
        ?procedure rdf:type obo:OGMS_0000112 .
        ?procedure dc:provenance ?reference. 
      }

PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
      PREFIX ddiem: <http://ddiem.phenomebrowser.net/>
      PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
      PREFIX owl: <http://www.w3.org/2002/07/owl#>
      PREFIX dc: <http://purl.org/dc/elements/1.1/>
      PREFIX obo: <http://purl.obolibrary.org/obo/>
      
      SELECT (count(distinct ?phenotype) as ?phenotype_count) (count(distinct ?phenotypeMention) as ?phenotypeMention_count)
      FROM <http://ddiem.phenomebrowser.net>
      WHERE {
        ?phenotype rdf:type ddiem:Phenotype . 
        ?phenotypeMention obo:RO_0002212 ?obj 
      }

PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
      PREFIX ddiem: <http://ddiem.phenomebrowser.net/>
      PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
      PREFIX owl: <http://www.w3.org/2002/07/owl#>
      PREFIX dc: <http://purl.org/dc/elements/1.1/>
      PREFIX obo: <http://purl.obolibrary.org/obo/>
   Select (count(*) as ?evidence_comb_count) where {  
    {  SELECT count(*) as ?evidence_count 
      FROM <http://ddiem.phenomebrowser.net>
      WHERE {
         ?proceure obo:RO_0002558 ?evidenceMention 
      } group by ?proceure
    } .
    FILTER(?evidence_count > 1) 
}

PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
      PREFIX ddiem: <http://ddiem.phenomebrowser.net/>
      PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
      PREFIX owl: <http://www.w3.org/2002/07/owl#>
      PREFIX dc: <http://purl.org/dc/elements/1.1/>
      PREFIX obo: <http://purl.obolibrary.org/obo/>
   Select (sum(*) as ?class_comb_count) where {  
    {  SELECT count(*) as ?class_count 
      FROM <http://ddiem.phenomebrowser.net>
      WHERE {
         ?proceure rdf:type ddiem:TheraputicProcedure .
         ?proceure rdfs:subClassOf ?class 
      } group by ?proceure
    } .
    FILTER(?class_count > 0) 
}

Select (sum(*) as ?count) where {  
{
SELECT count(*) as ?drugMention_count
      FROM <http://ddiem.phenomebrowser.net>
      WHERE {
        ?drug rdf:type rdf:Alt . 
        ?drug ?prop ?drugMention .
        FILTER(?prop != rdf:type)
      } group by ?drug
} .
FILTER(?drugMention_count > 0) 
}

      
SELECT (count(*) as ?phenotype_count) 
FROM <http://ddiem.phenomebrowser.net>
WHERE {
?phenotypeMention obo:RO_0002212 ?obj 
}

//Total drug mentions
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
      PREFIX ddiem: <http://ddiem.phenomebrowser.net/>
      PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
      PREFIX owl: <http://www.w3.org/2002/07/owl#>
      PREFIX dc: <http://purl.org/dc/elements/1.1/>
      PREFIX obo: <http://purl.obolibrary.org/obo/>
      

Select (sum(*) as ?count) where {  
{
SELECT count(*) as ?drugMention_count
      FROM <http://ddiem.phenomebrowser.net>
      WHERE {
        ?procedure obo:RO_0000057 ?drug .
        ?drug rdf:type rdf:Alt . 
        ?drug ?prop ?drugMention .
        FILTER(?prop != rdf:type)
      } group by ?procedure ?drug
} .
FILTER(?drugMention_count > 0) 
}

SELECT (count(distinct ?gene) as ?gene_count) 
      FROM <http://ddiem.phenomebrowser.net>
      WHERE {
        ?gene rdf:type ddiem:Gene .
      }
SELECT (count(*) as ?gene_mention_count) 
FROM <http://ddiem.phenomebrowser.net>
WHERE {
?disease obo:RO_0004020 ?obj 
}
#evidence mention by evidence code
SELECT ?evidenceMention count(*) as ?evidence_count 
FROM <http://ddiem.phenomebrowser.net>
WHERE { 
 ?proceure obo:RO_0002558 ?evidenceMention .
 ?evidenceMention rdf:type ddiem:Evidence .
} group by ?evidenceMention

SELECT ?class count(*) as ?class_count 
      FROM <http://ddiem.phenomebrowser.net>
      WHERE { 
         ?proceure rdf:type obo:OGMS_0000112 .
         ?proceure rdf:type ?class.
      } group by ?class


PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
    PREFIX ddiem: <http://ddiem.phenomebrowser.net/>
    PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
    PREFIX obo: <http://purl.obolibrary.org/obo/>
    PREFIX owl: <http://www.w3.org/2002/07/owl#>
      PREFIX dc: <http://purl.org/dc/elements/1.1/>
      PREFIX obo: <http://purl.obolibrary.org/obo/>
SELECT (count(distinct ?phenotype) as ?protein_without_id_count) 
      FROM <http://ddiem.phenomebrowser.net>
      WHERE { 
         ?phenotype rdf:type ddiem:Phenotype .
         MINUS { ?phenotype dc:identifier ?phenotypeID . }
      } 
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
      PREFIX ddiem: <http://ddiem.phenomebrowser.net/>
      PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
      PREFIX owl: <http://www.w3.org/2002/07/owl#>
      PREFIX dc: <http://purl.org/dc/elements/1.1/>
      PREFIX obo: <http://purl.obolibrary.org/obo/>
      
SELECT (count(distinct ?gene) as ?protein_mention_count) 
      FROM <http://ddiem.phenomebrowser.net>
      WHERE {
        ?disease obo:RO_0004020 ?gene . 
      }
# super class hp
    PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
      PREFIX ddiem: <http://ddiem.phenomebrowser.net/>
      PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
      PREFIX owl: <http://www.w3.org/2002/07/owl#>
      PREFIX dc: <http://purl.org/dc/elements/1.1/>
      PREFIX obo: <http://purl.obolibrary.org/obo/>
      
      SELECT count(distinct ?label)
      FROM <http://ddiem.phenomebrowser.net>
      WHERE {
        ?phenotypeMention obo:RO_0002212 ?obj .
        ?obj rdfs:label ?label .
        FILTER regex(str(?label), "[\\w\\d\\s]+\\([\\w\\d\\s]+\\)\\*") 
      }