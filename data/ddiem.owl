    <!-- http://purl.obolibrary.org/obo/DDIEM_0000001 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000001">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OGMS_0000112"/>
        <IAO_0000115 xml:lang="en">A treatment strategy or plan which the clinician believes will cure or ameliorate a disease caused by a deleterious genetic variation in a patient, affecting their metabolism. It may modify some or all of its signs or symptoms, or alter the prognosis and course of the disease, either by severity, timecourse, or phenotypic quality.</IAO_0000115>
        <dc:creator xml:lang="en">ORCID: 0000-0002-5111-7263</dc:creator>
        <rdfs:label xml:lang="en">metabolic disease therapeutic procedure</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000002 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000002">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DDIEM_0000005"/>
        <IAO_0000115 xml:lang="en">A therapeutic procedure in which a treatment directly or indirectly compensates for the loss or gain of activity of a genetically defective protein within the network or pathway of which that protein is a member.</IAO_0000115>
        <dc:creator>ORCID: 0000-0002-5111-7263</dc:creator>
        <rdfs:label xml:lang="en">complementation therapy</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000004 -->
 <!-- Pegylated recombinant phenylalanine ammonia lyase in treating OMIM disease # 261600, Enzyme replacement therapy https://www.ncbi.nlm.nih.gov/pubmed/28282402 -->
    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000004">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DDIEM_0000002"/>
        <IAO_0000115 xml:lang="en">A therapeutic procedure in which a genetically defective protein is replaced by a canonical source of the same protein, genetically as in gene therapy, or by some other means of delivery, whose completion is hypothesized by a health care provider to eliminate a disorder or to alleviate the signs and symptoms of a disorder or pathological process.</IAO_0000115>
        <dc:creator>ORCID: 0000-0002-5111-7263</dc:creator>
        <dc:creator>leechuck</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-07-15T10:19:07Z</dc:date>
        <rdfs:label xml:lang="en">direct complementation of a genetically defective protein</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000005 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000005">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DDIEM_0000001"/>
        <IAO_0000115 xml:lang="en">A therapeutic procedure which directly addresses the effects of a protein defect produced by the mutation, its structure, activity, or end product. Therapies which target the local interaction or regulatory network or pathway in which the affected protein lies are also regarded as mechanistic therapies.</IAO_0000115>
        <dc:creator>ORCID: 0000-0002-5111-7263</dc:creator>
        <dc:creator>leechuck</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-07-15T17:59:32Z</dc:date>
        <rdfs:label xml:lang="en">mechanistically predicated therapeutic procedure</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000006 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000006">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DDIEM_0000002"/>
        <IAO_0000115 xml:lang="en">A therapeutic procedure in which the availability, activity, stability, or turnover of a defective enzyme is modified by delivery of small or macro-molecules, or genetic or epigenetic manipulation.</IAO_0000115>
        <dc:creator xml:lang="en">ORCID: 0000-0002-5111-7263</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-07-15T18:02:30Z</dc:date>
        <rdfs:label xml:lang="en">compensatory complementation of a genetically defective protein</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000008 -->
<!-- Fat-limited diet+ High carbohydrate diet in treating OMIM disease #248360 low-fat/high-carbohydrate diet treatment is highly effective https://www.ncbi.nlm.nih.gov/pubmed/28781843 --> 
    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000008">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DDIEM_0000005"/>
        <IAO_0000115 xml:lang="en">A therapeutic procedure in which the diet is supplemented with or depleted in molecules closely related to the products or end processes of a genetically defective protein which occur as natural products in the diet.</IAO_0000115>
        <dc:creator>ORCID: 0000-0002-5111-7263</dc:creator>
        <dc:creator>leechuck</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-07-15T18:03:52Z</dc:date>
        <rdfs:label xml:lang="en">dietary regime modification</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000009 -->
    <!-- valine-restricted diet in treating OMIM disease #611283 in which the defected enzyme" isobutyryl-CoA dehydrogenase (IBD)" is involved in the degradation of valine. https://www.ncbi.nlm.nih.gov/pubmed/16857760?dopt=Abstract&holding=npg --> 
    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000009">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DDIEM_0000008"/>
        <IAO_0000115 xml:lang="en">A therapeutic procedure in which the diet is depleted in molecules closely related to the products or end processes of a genetically defective protein, which occur naturally in the diet.</IAO_0000115>
        <dc:creator>ORCID: 0000-0002-5111-7263</dc:creator>
        <dc:creator>leechuck</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-07-15T18:04:24Z</dc:date>
        <rdfs:label xml:lang="en">dietary exclusion</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000010 -->
<!-- Uncooked Cornstarch in treating OMIM disease # 227810 Uncooked cornstarch provides a substrate for slow enteric absorption of carbohydrate, thereby prolonging the duration of euglycemia between meals.https://www.ncbi.nlm.nih.gov/pubmed/24912437 -->
    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000010">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DDIEM_0000008"/>
        <IAO_0000115 xml:lang="en">A therapeutic procedure in which the diet is supplemented with foodstuffs containing molecules closely related to the products or end processes of a genetically defective protein, which occur naturally in the diet.</IAO_0000115>
        <dc:creator>ORCID: 0000-0002-5111-7263</dc:creator>
        <dc:creator>leechuck</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-07-15T18:04:54Z</dc:date>
        <rdfs:label xml:lang="en">dietary supplementation</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000011 -->
<!-- Niacin in treating OMIM disaese # 234500 that affects the absorption of nonpolar amino acids (particularly tryptophan that can be, in turn, converted into serotonin, melatonin, and niacin.https://www.ncbi.nlm.nih.gov/pubmed/6236384 -->
    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000011">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DDIEM_0000006"/>
        <IAO_0000115 xml:lang="en">A therapeutic procedure involving enteral, parenteral, or transdermal provision of small molecules closely related to the products or end processes of a genetically defective protein.</IAO_0000115>
        <dc:creator>ORCID: 0000-0002-5111-7263</dc:creator>
        <dc:creator>leechuck</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-07-15T18:06:25Z</dc:date>
        <rdfs:label xml:lang="en">metabolite replacement</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000012 -->
<!-- Tanespimycin  in treating OMIM disease # 602522 17-allylamino-17-demethoxygeldanamycin (17-AAG), an Hsp90 inhibitor, enhanced the plasma membrane expression of mutant barttins (R8L and G47R) in Madin–Darby canine kidney cells. https://www.ncbi.nlm.nih.gov/pubmed/24189473 -->
   <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000012">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DDIEM_0000006"/>
        <IAO_0000115 xml:lang="en">A therapeutic procedure in which generally small molecules are delivered to the organism in order to directly increase, decrease, or alter the activity or stability of a genetically defective protein.</IAO_0000115>
        <dc:creator>ORCID: 0000-0002-5111-7263</dc:creator>
        <dc:creator>leechuck</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-07-15T18:07:23Z</dc:date>
        <rdfs:label xml:lang="en">activity modification of a genetically defective protein</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000013 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000013">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DDIEM_0000001"/>
        <IAO_0000115 xml:lang="en">A therapeutic procedure aimed at amelioration of one or more abnormal phenotypes generated as a consequence of a defective protein or process by a means unrelated to the immediate pathway or network environment of the defective protein. Generally working at the level of the tissue or overall organismal physiology.</IAO_0000115>
        <dc:creator>ORCID: 0000-0002-5111-7263</dc:creator>
        <dc:creator>leechuck</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-07-15T18:07:49Z</dc:date>
        <rdfs:label xml:lang="en">symptomatic therapeutic procedure</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000014 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000014">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DDIEM_0000001"/>
        <IAO_0000115 xml:lang="en">A therapeutic procedure to mitigate the immediate or future effects of the presence of a genetically defective protein which involves physical conditioning or anatomical modification.</IAO_0000115>
        <dc:creator>ORCID: 0000-0002-5111-7263</dc:creator>
        <dc:creator>leechuck</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-07-15T18:08:30Z</dc:date>
        <rdfs:label xml:lang="en">surgical or physical therapeutic procedure</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000015 -->
<!--  Rapamycin in treating OMIM # 232400 rapamycin significantly reduced glycogen content in primary muscle cells from human patients with GSD IIIa by suppressing the expression of glycogen synthase and glucose transporter 1. https://www.ncbi.nlm.nih.gov/pubmed/24509886 -->
    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000015">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DDIEM_0000021"/>
        <IAO_0000115 xml:lang="en">A therapeutic procedure that inhibits a component of the pathway or network in which a genetically defective protein is found, compensating for alteration of activity of that protein.</IAO_0000115>
        <dc:creator>ORCID: 0000-0002-5111-7263</dc:creator>
        <dc:creator>leechuck</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-07-15T18:08:56Z</dc:date>
        <rdfs:label xml:lang="en">functional complementation of a genetically defective protein by inhibition</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000016 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000016">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DDIEM_0000021"/>
        <IAO_0000115 xml:lang="en">A therapeutic procedure that stimulates a component of the pathway or network in which a genetically defective protein is found, compensating for alteration of activity of that protein.</IAO_0000115>
        <dc:creator>ORCID: 0000-0002-5111-7263</dc:creator>
        <dc:creator>leechuck</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-07-15T18:09:20Z</dc:date>
        <rdfs:label xml:lang="en">functional complementation of a genetically defective protein by stimulation</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000017 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000017">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DDIEM_0000012"/>
        <IAO_0000115 xml:lang="en">Modulation of the level or specificity of expression of a defective gene through modification of an epigenetic mark or process using chemical, biological, or genetic means.</IAO_0000115>
        <dc:creator>ORCID: 0000-0002-5111-7263</dc:creator>
        <rdfs:label xml:lang="en">activity modification of a genetically defective protein by epigenetic manipulation</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000018 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000018">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DDIEM_0000012"/>
        <IAO_0000115 xml:lang="en">Modulation of the activity or stability of a genetically defective gene through direct modification of one or both endogenous alleles of the gene, using genome editing techniques such as CRISPR/Cas.</IAO_0000115>
        <dc:creator>ORCID: 0000-0002-5111-7263</dc:creator>
        <rdfs:label xml:lang="en">activity modification of a genetically defective protein by genome editing</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000019 -->
<!-- phenobarbital in treating OMIM disease# 606785 the drug induces production of UGT1A1 https://www.ncbi.nlm.nih.gov/pubmed/6872808 -->
    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000019">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DDIEM_0000012"/>
        <IAO_0000115>Any procedure designed to suppress or mitigate the effects of a recessive genetic change in an abnormal protein by modification of its transcription or translation, excluding those strategies which involve epigenetic modification or modification of the germline or somatic genome sequence. For example, an abnormal Vmax of reaction, alteration of KM, failure to respond to positive feedback, or acquisition of neofunction.</IAO_0000115>
        <dc:creator>ORCID: 0000-0002-5111-7263</dc:creator>
        <dc:date>3.10.19</dc:date>
        <rdfs:label xml:lang="en">activity modification of a genetically defective protein by transcriptional or translational modification</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000020 -->
<!-- AAV2-hAADC in treating OMIM disease # 608643 https://clinicaltrials.gov/ct2/show/NCT02926066?cond=Aromatic+L-amino+acid+decarboxylase+deficiency&rank=2 -->
    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000020">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DDIEM_0000004"/>
        <IAO_0000115 xml:lang="en">Insertion of one or more normal copies of a genetically defective gene into the genome of affected cells.</IAO_0000115>
        <dc:creator>ORCID: 0000-0002-5111-7263</dc:creator>
        <rdfs:label xml:lang="en">direct complementation of a genetically defective protein by gene therapy</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000021 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000021">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DDIEM_0000002"/>
        <IAO_0000115 xml:lang="en">A therapeutic procedure in which the composition or metabolic activity of the pathway or network in which the defective protein is found is modified, compensating for alteration of activity of that protein.</IAO_0000115>
        <dc:creator>ORCID: 0000-0002-5111-7263</dc:creator>
        <dc:creator>leechuck</dc:creator>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2019-07-15T18:03:22Z</dc:date>
        <rdfs:label xml:lang="en">functional complementation of a genetically defective protein</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000022 -->
<!--Dexamethasone in treating OMIM disease # 162300 Glucocorticoids differentially inhibit expression of the RET proto-oncogene.https://www.ncbi.nlm.nih.gov/pubmed/10947080 -->
    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000022">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DDIEM_0000005"/>
        <IAO_0000115>A procedure to suppress or mitigate the effects of a dominant alteration in a genetically abnormal protein by reducing or suppressing transcription or translation of the abnormal protein gene or rna, or destabilising or otherwise inhibiting an abnormally increased activity or neoactivity of the abnormal protein.</IAO_0000115>
        <dc:creator>ORCID: 0000-0002-5111-7263</dc:creator>
        <dc:date>4.10.19</dc:date>
        <rdfs:label xml:lang="en">procedure to mitigate dominant effects of a genetically abnormal protein</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/DDIEM_0000023 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DDIEM_0000023">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OGMS_0000112"/>
        <IAO_0000115 xml:lang="en">A therapeutic procedure using multiple therapies to treat a single disease. Often all the therapies are pharmaceutical. It can also involve non-drug therapy, such as the combination of medications, behaviour modification, or physical procedures. The different components might act through the same or different mechanisms.</IAO_0000115>
        <dc:creator xml:lang="en">ORCID: 0000-0002-5111-7263</dc:creator>
        <dc:date xml:lang="en">2019-12-10</dc:date>
        <rdfs:label xml:lang="en">combination therapeutic procedure</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/OGMS_0000112 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OGMS_0000112"/>
</rdf:RDF>



